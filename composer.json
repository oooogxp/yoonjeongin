{
  "require": {
    "craftcms/cms": "3.6.16",
    "craftcms/feed-me": "4.3.6",
    "craftcms/redactor": "2.8.7",
    "doublesecretagency/craft-inventory": "2.1.1",
    "focuslabllc/craft-cheat-sheet": "2.0.3",
    "fortrabbit/craft-auto-migrate": "^2.2",
    "misterbk/mix": "1.5.2",
    "nystudio107/craft-emptycoalesce": "^1.0",
    "nystudio107/craft-minify": "^1.2",
    "nystudio107/craft-retour": "3.1.53",
    "nystudio107/craft-seomatic": "3.3.42",
    "nystudio107/craft-twigpack": "1.2.14",
    "spicyweb/craft-neo": "2.9.10",
    "studioespresso/craft-dumper": "2.0.0",
    "topshelfcraft/wordsmith": "3.3.0.1",
    "verbb/expanded-singles": "1.1.3",
    "verbb/field-manager": "2.2.2",
    "verbb/formie": "1.4.4",
    "verbb/knock-knock": "1.2.14",
    "verbb/navigation": "1.4.16",
    "verbb/super-table": "2.6.8",
    "vlucas/phpdotenv": "^3.4.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
